{
  "expo": {
    "name": "junaid",
    "slug": "junaid",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "appnotesnative",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.anonymous.appnotesnative"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      ["expo-router"],
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": { 
      "EXPO_PUBLIC_APPWRITE_PROJECT_ID": "junaid01",
      "EXPO_PUBLIC_APPWRITE_ENDPOINT": "https://fra.cloud.appwrite.io/v1",
      "EXPO_PUBLIC_APPWRITE_DB_ID": "junaid01",
      "EXPO_PUBLIC_APPWRITE_COL_NOTES_ID": "junaid",
      "EXPO_PUBLIC_APPWRITE_BUNDLE_ID": "junaid-ios",
      "EXPO_PUBLIC_APPWRITE_PACKAGE_NAME": "junaid-android"
    }
  }
}
